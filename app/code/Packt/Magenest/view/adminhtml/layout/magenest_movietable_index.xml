<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceContainer name="content">
            <block class="Packt\Magenest\Block\Adminhtml\Movietable"
                   name="adminhtml.block.magenest.movietable.container">
                <block class="Packt\Magenest\Block\Adminhtml\Movietable\Grid"
                       name="adminhtml.block.magenest.movietable.grid" as="grid" />
            </block>
        </referenceContainer>
        <referenceContainer name="page.main.actions">
            <block class="Magento\Backend\Block\Template"
                   name="component_buttonmovie" template="Packt_Magenest::component/toolbar/buttonmovie.phtml"/>
        </referenceContainer>

    </body>
</page>